{
  "name": "ajaxious",
  "version": "2.8.4",
  "description": "It is easier to use 'ajaxious' instead of 'fetch' or 'axios'.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server -d --env.dev",
    "build": "npm run clean & webpack -p --env.prod",
    "postbuild": "tsc --emitDeclarationOnly --declaration",
    "deploy": "npm run build & npm run postbuild & npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soorena110/Ajaxious.git"
  },
  "keywords": [
    "ajax",
    "fetch",
    "axios",
    "ajaxious",
    "fetch wrapper",
    "event driven request",
    "aspect programing"
  ],
  "author": "MohammadReza Azarang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soorena110/Ajaxious/issues"
  },
  "homepage": "https://github.com/soorena110/Ajaxious#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.7.1",
    "azi-tools": "0.1.1"
  },
  "dependencies": {
    "promise-polyfill": "8.1.3",
    "whatwg-fetch": "^3.0.0"
  }
}
